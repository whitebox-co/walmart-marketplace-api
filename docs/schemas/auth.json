                }
              },
              "application/xml": {
                "schema": {
                  "required": [
                    "accessToken"
                  ],
                  "type": "object",
                  "properties": {
                    "accessToken": {
                      "type": "string",
                      "description": "Unique token string generated by the API call",
                      "xml": {
                        "namespace": "http://walmart.com/"
                      }
                    },
                    "tokenType": {
                      "type": "string",
                      "description": "Type of token according to user. (e.g., 'BEARER')",
                      "xml": {
                        "namespace": "http://walmart.com/"
                      }
                    },
