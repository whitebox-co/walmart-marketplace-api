  <xsd:complexType name="shippingOverride">
    <xsd:all>
      <xsd:element maxOccurs="1" minOccurs="1" name="ShippingOverrideIsShippingAllowed">
        <xsd:annotation>
          <xsd:documentation>Shipping may not apply to some items. Please select a value from the drop-down below.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Shipping Override - isShippingAllowed</wm:displayName>
            <wm:group>Offer</wm:group>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Yes"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="ShippingOverrideShipMethod">
        <xsd:annotation>
          <xsd:documentation>Specifies the ship method for the corresponding region to be overridden. Please select a value from the drop-down below.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Shipping Override - Ship Method MP</wm:displayName>
            <wm:group>Offer</wm:group>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="VALUE"/>
            <xsd:enumeration value="STANDARD"/>
            <xsd:enumeration value="EXPEDITED"/>
            <xsd:enumeration value="FREIGHT"/>
            <xsd:enumeration value="ONE_DAY"/>
            <xsd:enumeration value="FREIGHT_WITH_WHITE_GLOVE"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="ShippingOverrideShipRegion">
        <xsd:annotation>
          <xsd:documentation>Select the region where you are willing to ship your product.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Shipping Override - Ship Region</wm:displayName>
            <wm:group>Offer</wm:group>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
