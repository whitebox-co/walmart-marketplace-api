            <xsd:maxLength value="400"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="hatSize">
        <xsd:annotation>
          <xsd:documentation>The sizing information specific to hats, ranging from 6 1/4 to 8 1/8. Generic sized (small/med/large) hats may not have this information (generic s/m/l size information should be entered under the &quot;Clothing Size&quot; attribute).</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Hat Size</wm:displayName>
            <wm:group>Upper Body Features</wm:group>
            <wm:rank>71000</wm:rank>
          </xsd:appinfo>
