        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="6 1/4"/>
            <xsd:enumeration value="6 3/8"/>
            <xsd:enumeration value="6 1/2"/>
            <xsd:enumeration value="6 5/8"/>
            <xsd:enumeration value="6 3/4"/>
            <xsd:enumeration value="6 7/8"/>
            <xsd:enumeration value="7"/>
            <xsd:enumeration value="7 1/8"/>
            <xsd:enumeration value="7 1/4"/>
            <xsd:enumeration value="7 3/8"/>
            <xsd:enumeration value="7 1/2"/>
            <xsd:enumeration value="7 5/8"/>
            <xsd:enumeration value="7 3/4"/>
            <xsd:enumeration value="7 7/8"/>
            <xsd:enumeration value="8"/>
            <xsd:enumeration value="8 1/8"/>
