        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="jacketStyle">
        <xsd:annotation>
          <xsd:documentation>Styles specific to coats and jackets.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Jacket Style</wm:displayName>
