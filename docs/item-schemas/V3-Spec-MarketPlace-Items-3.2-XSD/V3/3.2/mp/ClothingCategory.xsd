            <xsd:enumeration value="single-breasted"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="sweaterStyle">
        <xsd:annotation>
          <xsd:documentation>Styles specific to sweaters.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Sweater Style</wm:displayName>
            <wm:group>Upper Body Features</wm:group>
            <wm:rank>68000</wm:rank>
          </xsd:appinfo>
