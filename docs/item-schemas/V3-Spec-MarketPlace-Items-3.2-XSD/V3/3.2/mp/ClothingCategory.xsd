      <xsd:element maxOccurs="1" minOccurs="0" name="sleeveLengthStyle">
        <xsd:annotation>
          <xsd:documentation>Descriptive terms for sleeve length.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Sleeve Length Style</wm:displayName>
